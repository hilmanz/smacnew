/*------------------------------Table Style-----------------------------*/
table.list td,table.addlist td {	
	border-bottom: solid 1px #eaeaea;
	padding:8px;
}
table.list tr:hover {
	background-color:#666666;
	background-image:none;
	color:#FFF;
	text-shadow:none;
}
table.list tr:hover a {
		color: #FF0;
	}
table.list,table.addlist, table.zebra {
	border: solid 1px #eaeaea;
	border-bottom:none;
	width:100%;
}
tr.ganjil, tr.oddRow {
	background:#f9f9f9;
}
tr.genap, tr.evenRow {
	background:#f3f3f3;
}
table.zebra {
	border-right:none;
}
table.zebra td {
	border-right:solid 1px #eaeaea;
}
tr.head, td.head {
	height:27px;
	background:url(images/table.gif) repeat-x;
	text-shadow:1px 1px 1px #cccccc;
	font-weight:bold;
}

input[type=file] {
}

/*-----------------------login---------------------*/

#login {
	margin:150px auto;
	background-color:#ffffff;
	border:solid 2px #0d457e;
	width:300px;
	height:200px;
	padding:20px;
	-moz-box-shadow:0 2px 14px 0 #333333;
	-moz-border-radius:15px;
	display:block;
	color:#002d5b;
	text-shadow:1px 1px 1px #cccccc;
	text-transform:uppercase;
	text-align:left;
}
#login h1 {
	font-weight:bold;
	font-size:15px;
	margin:0 0 10px 0;
	color:#000;
}
#login p {
	margin:0;
	color:#666;
}
.messageLogin {
	color:#CC0000;
	margin:0;
	float:right;
	font-size:10px;
	padding:15px 0 0 0;
}
#login #button{
		background-image:none;
		background-color:#949494;
		color:#FFF;
		text-transform:uppercase;
		width:80px;
		text-align:center;
	}
#login #password, #login #username{
		width:280px;
		background: url(images/bg_input_login.jpg) no-repeat;
		border:none;
		padding:5px 10px;
	}
#login label{
		display:block;
		padding:10px 0;
		width:100%;
		color:#666;
	}	

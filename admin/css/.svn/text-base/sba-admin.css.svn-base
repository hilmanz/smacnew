/*
Theme Name:SBA
Author: Acit Jazz 
Version: 1.0
*/
/*---------------------main----------------------*/
body{
	background:url(../images/bg_gradient.png) repeat-x #fff;
	font-family:"Trebuchet MS", Arial, Helvetica, sans-serif;
	font-size:12px;
	color:#333;
	margin:0;
	padding:0;
	line-height:18px;
}
#wrapper{
	width:994px;
	margin:0 auto;
	min-height:600px;
	overflow:hidden;
}
#header{
	background:url(../images/bg_header.png) no-repeat;
	height:174px;
}
#container{
	background:url(../images/bg_container.png) repeat-y;
	width:970px;
	min-height:600px;
	overflow:hidden;
	padding:0 12px;
}
#main-content{
	background:url(../images/bg.jpg) bottom repeat-x #fff;
	min-height:600px;
	overflow:hidden;
	padding:0 0 20px 0;
}
#nav{
	background:#cccccc;
	height:30px;
	padding:10px 10px 0 10px;
}
#banner{
	width:970px;
	height:170px;
}
#footer{
	background:url(../images/bg_footer.png) no-repeat;
	height:140px;
	padding:20px 0 0 0;
	text-align:center;
}

#footer a{
	padding:0 5px;
}
/*---------------------Element----------------------*/
a img{
	border:none;
}
a{
	color:#F00;
	outline:none;
}
a:hover{
	color:#900;
}
.date{
	color:#F00;
	font-weight:bold;
}
.time{
	color:#999999;
	display:block;
}
.img-left{
	float:left;
	margin:5px 10px 0 0;
}
.small-thumb{
	float:left;
	margin:6px 20px 0 0;
}
.username{
	font-weight:bold;
	display:block;
}
.percent{
	display:block;
	color:#999;
}
form{
	margin:0;
	padding:0;
}
/*---------------------navigation----------------------*/
#nav .nav-left,#nav .nav-right{
	margin:0;
	padding:0;
	list-style:none;
	float:left;
}
#nav .nav-right{
	float:right;
}
#nav .nav-left li{
	float:left;
}
#nav .nav-left li a{
	color:#333;
	text-decoration:none;
	padding:5px 15px;
}
#nav .nav-right li{
	float:right;
}
#nav .nav-right li a{
	color:#F00;
	padding:5px 15px;
	text-decoration:none;
}
#nav .nav-right li a:hover,#nav .nav-left li a:hover{
	background:#900;
	color:#fff;
	-moz-border-radius:10px;
	-webkit-border-radius:10px;
	border-radius:10px;
}

/*---------------------TITLE BAR----------------------*/
#title-bar{
	clear:both;
	height:30px;
	padding:20px;
}
#title-bar h1{
	float:left;
	margin:0;
	padding:0;
	font-size:20px;
}
#title-bar .info{
	float:right;
}
#title-bar .info .welcome{
	font-size:14px;
	padding:0 5px 0 0;
}
#title-bar .info .username{
	color:#F00;
	font-weight:bold;
	display:inline;
	font-size:14px;
}
/*---------------------SIDE LEFT----------------------*/
#col-left,#col-right,#w200,#w650{
	width:240px;
	float:left;
	min-height:400px;
	padding:0 20px;
	overflow:hidden;
}
#w200{
	width:160px;
}
#w650{
	width:650px;
	padding:0 20px;
}
#col-left .title,#col-center .title,#col-right .title,#footer-content .title{
	background:url(../images/icon_title.jpg) left no-repeat;
	padding:7px 0 0 18px;
	border-bottom:dotted 1px #ccc;
	clear:both;
	height:25px;
}
#col-left h1, #col-center h1, #col-right h1,#footer-content h1{
	font-size:14px;
	color:#ff0000;
	margin:0;
	padding:0;
	float:left;
	text-transform:uppercase;
}
#col-left a.view-all,#col-center a.view-all,#col-right a.view-all,#footer-content a.view-all{
	float:right;
	background:url(../images/arrow.png) right no-repeat;
	padding:0 10px 0 0;
	color:#000;
	font-size:11px;
	text-decoration:none;
}
#col-left  .content{
	padding:20px 0;
}
#col-left  .content .list{
	background:url(../images/icon_list.jpg) bottom right no-repeat #ccc;
	height:75px;
	margin:0 0 15px 0;
}
#col-left  .content .list img{
	float:left;
	margin:0 10px 0 0;
}
#col-left  .content .list a,#col-left  .content .list span,#col-right  .content .list .text span{
	display:block;
	text-decoration:none;
	padding:0;
	color:#000;
}
#col-left  .content .list a:hover,#col-left a.view-all:hover,#col-center a.view-all:hover,#footer-content a.view-all:hover{
	color:#F00;
}
#col-left  .content .list a.username,#col-left  .content .list a.percent, #col-left  .content .list a.title{
	font-weight:bold;
	display:inline;
	background:none;
}
#col-left  .content .list .text{
	padding:5px 15px 0 0;
	color:#000;
}
#col-left .info-profile{
	padding:10px 0 20px 0;
}
#col-left .info-profile span,.info-profile span{
	display:block;
}
.info-profile .title{
	font-weight:bold;
}
.info-profile .total-reg{
	font-size:16px;
	padding:0 0 10px 0;
}
.profile-pic{
	min-height:80px;
	overflow:hidden;
}
.info-profile{
	padding:10px 0;
}
.info-profile .kota{
	font-weight:bold;
}
.nav-user{
	padding:10px 0 0 0;
}
.nav-user a{
	display:block;
	padding:5px 0;
	border-bottom:dashed 1px #ccc;
}
/*---------------------SIDE CENTER----------------------*/
#col-center{
	margin:0 0 0 0;
	width:410px;
	float:left;
}
#col-center .content .text p{
	margin:0;
}
#col-center .download{
	margin:0;
	clear:both;
	height:70px;
	background:url(../images/download.jpg) no-repeat 10px 20px;
	padding:10px 10px 0 50px;
	display:block;
	text-decoration:none;
	color:#333;
}
#col-center .info{
	display:block;
}
#col-center .bold{
	display:block;
	font-weight:bold;
}
#col-center .network-update{
	display:block;
	clear:both;
	padding:15px 0;
}

#col-center .network-update,#col-center .comments {
	display:block;
	clear:both;
	padding:15px 0;
}
#col-center .text-share{
	width:400px;
	padding:5px;
	height:35px;
	border:solid 1px #ccc;
}
#col-center .submit-share{
	background:#F00;
	border:solid 1px #900;
	color:#fff;
	padding:4px 10px;
	cursor:pointer;
	float:right;
	margin:-1px -1px 0 0;
}
#col-center .network-update .text,#col-center .comments .text{
	padding:0 0 0 65px;
}
#col-center .comments{
	border-bottom:dashed 1px #ccc;
}
#col-center .text .list span{
	display:block;
	clear:both;
}
#col-center .tab{
	margin:12px 0 0 0;
}
#col-center h1.news-title,#col-center h1.news-title a{
	float:none;
	margin:0 0 15px 0;
	clear:both;
	color:#333;
	text-decoration:none;
	display:block;
}
#col-center h1.news-title a:hover{
	color:#F00;
}
#col-center .comment{
	display:block;
}
#col-center .like{
	display:block;
}
.form-comment{
	margin:0;
	padding:20px 0;
}
.form-comment textarea{
	width:400px;
	padding:5px;
	border:solid 1px #ccc;
}
.form-comment input[type=submit],.rsvp-button{
	border:solid 1px #900;
	color:#fff;
	background:#F00;
	padding:3px 10px;
	float:right;
	margin:-1px -1px 0 0;
	cursor:pointer;
}
.form-comment input[type=submit]:hover,.rsvp-button:hover{
	background:#900;
	color:#fff;
}
.entry-detail{
	clear:both;
	padding:0 0 20px 0;
}
a.comment{
	background:url(../images/comment_icon.gif) no-repeat;
	display:block;
	width:80px; height:20px;
	padding:0 0 0 25px;
	font-size:11px;
	text-decoration:none;
	text-indent:2px;
	float:left;
}

a.like{
	background:url(../images/like_icon.gif) no-repeat;
	display:block;
	width:80px; height:20px;
	padding:0 0 0 25px;
	font-size:11px;
	text-decoration:none;
	text-indent:2px;
	float:left;
}
.icon{
	display:block;
	clear:both;
	padding:10px 0;
}
.network-update{
	border-bottom:dashed 1px #ccc;
}
.rsvp{
	display:block;
	clear:both;
	padding:10px 0;
	border-top: dashed 1px #ccc;
}
.rsvp-button{
	text-decoration:none;
	float:none;
}
.rsvp .list .small-thumb{
	margin:0 10px 10px 0;
}
.total{
	padding:0 5px 0 0;
	font-size:14px;
	color:#F00;
}
.downloadlist,.select-user{
	float:right;
	border:solid 1px #CCC;
	width:130px;
	padding:2px;
}
.galery-list{
	width:150px;
	text-align:center;
	float:left;
	padding:10px;
}
.galery-list .desc{
	display:block;
}
.galery-list .size{
	font-weight:bold;
}
#col-center .galery-list a.download{
	background:none;
	padding:0;
	height:auto;
	margin:0;
	color:#F00;
	text-decoration:underline;
}
.paging{
	padding:10px 0;
	clear:both;
}
.paging .next{
	float:right;
	background:url(../images/arrow.png) right no-repeat;
	padding:0 10px 0 10px;
	color:#000;
	font-size:11px;
	text-decoration:none;
}
.paging .prev{
	float:right;
	background:url(../images/arrow-back.png) left no-repeat;
	padding:0 10px 0 10px;
	color:#000;
	font-size:11px;
	text-decoration:none;
}
.content .titlebar h1{
	margin:10px 0;
	clear:both;
	display:block;
}
.content .titlebar{
	display:block;
	clear:both;
	padding:10px 0;
	height:15px;
}
/*---------------------SIDE RIGHT----------------------*/
#col-right .title{
	background:url(../images/icon_title2.jpg) 10px 11px no-repeat #bfbfbf;

	text-align:center;
	border:dashed 1px #ccc;
	padding:10px 5px;
	margin:0 10px 0 0;
	float:left;
}
.info-bar-overall .info-list{
	width:150px;
}
.info-list h1,.info-login h1{
	font-size:12px;
	margin:0;
	color:#F00;
}
.info-list p.point{
	font-size:36px;
	margin:10px 0;
}
.info-login{
	padding:40px 0 0 10px;
	float:left;
	width:160px;
}
.info-login p.date{
	margin:0;
	color:#666;
}
#w700{
	width:730px;
	padding:0 0 0 20px;
	float:left;
}
.red-bar{
	background:#C00;
	height:20px;
	padding:5px 15px;
}
.red-bar h1{
	color:#fff;
	font-size:12px;
	margin:0;
	float:left;
}
.red-bar .select-date{
	border:solid 1px #CCC;
	padding:2px;
	float:right;
	margin:-1px 8px 0 0;
}

.red-bar .go{
	float:right;
}
.grafik{
	padding:15px 0;
}
#w200 .list img{
	float:left;
}
#w200 .list{
	background:url(../images/icon_list.jpg) bottom right no-repeat #ccc;
	height:75px;
	margin:0 0 15px 0;
	clear:both;
}
#w200 .content{
	padding:20px 0;
}
#w200 .content .text{
	float:left;
	padding:0 0 0 10px;
}
#w200 .content .text a{
	color:#333;
	text-decoration:none;
}
#w200 .content .text a:hover{
	color:#F00;
}
/*------------------- Footer Content --------------------*/
#footer-content{
	clear:both;
	min-height:200px;
	padding:0 20px;
	overflow:hidden;
}
.gallery-scroll{
	width:800px;
	margin:0 auto;
}
/*------------------- scroll gallery --------------------*/
.scrollable {
	position:relative;
	overflow:hidden;
	width:700px;
	height:200px;
}
.scrollable .items {
	width:20000em;
	position:absolute;
	clear:both;
}
.scrollable a {
	float:left;
	margin:20px 5px 20px 21px;
	background-color:#fff;
	padding:2px;
	border:1px solid #ccc;
	cursor:pointer;
	width:140px;
	height:100px;
	text-align:center;
	text-decoration:none;
}
.scrollable a span{
	display:block;
	text-decoration:none;
	color:#333;
	padding:10px 0 0 0;
}
.scrollable .active {
	border:2px solid #000;
	z-index:9999;
	position:relative;
}
/* this makes it possible to add next button beside scrollable */
.scrollable {
	float:left;
}
/* prev, next, prevPage and nextPage buttons */
a.browse {
	background:url(../images/hori_large.png) no-repeat;
	display:block;
	width:30px;
	height:30px;
	float:left;
	margin:55px 10px;
	cursor:pointer;
	font-size:1px;
}
/* right */
a.right {
	background-position: 0 -30px;
	clear:right;
	margin-right: 0px;
}
a.right:hover {
	background-position:-30px -30px;
}
a.right:active {
	background-position:-60px -30px;
}
/* left */
a.left {
	margin-left: 0px;
}
a.left:hover {
	background-position:-30px 0;
}
a.left:active {
	background-position:-60px 0;
}




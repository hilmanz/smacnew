<?xml version="1.0" encoding="UTF-8"?>
<data>
	{section name=i loop=$list}
	<item>
		<id>{$list[i].id}</id>
		<title>{$list[i].title|stripslashes}</title>
		<summary>{$list[i].summary|stripslashes}</summary>
		<date>{$list[i].created_date|date_format:"%d-%m-%Y, %T %p"}</date>
	</item>	
	{/section}
</data>
